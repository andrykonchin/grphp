{
  "name": "bigcommerce/grphp",
  "type": "library",
  "description": "gRPC PHP Framework",
  "license": "MIT",
  "require": {
    "google/protobuf": "3.5.*",
    "grpc/grpc": "^1.13"
  },
  "require-dev": {
    "phpunit/php-code-coverage": "^5.2@dev",
    "phpunit/phpunit": "6.1.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "config": {
    "platform": {
      "php": "7.0.30"
    }
  },
  "autoload": {
    "psr-4": {
      "Grphp\\": "src/Grphp"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
